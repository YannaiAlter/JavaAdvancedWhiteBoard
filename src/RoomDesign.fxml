<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="CENTER" stylesheets="@RoomDesign.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RoomController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <BorderPane id="roomlist">
         <top>
            <Label prefHeight="45.0" prefWidth="96.0" text="Room List" textFill="WHITE" wrapText="true" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Avenir Next Bold" size="20.0" />
               </font>
               <BorderPane.margin>
                  <Insets top="20.0" />
               </BorderPane.margin>
            </Label>
         </top>
         <bottom>
            <GridPane prefHeight="534.0" prefWidth="518.0" BorderPane.alignment="CENTER">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints maxHeight="83.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="374.0" minHeight="10.0" prefHeight="374.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="138.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <TextArea id="outputChat" fx:id="outputChat" disable="true" prefHeight="354.0" prefWidth="390.0" GridPane.rowIndex="1">
                     <GridPane.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                     </GridPane.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <font>
                        <Font size="11.0" />
                     </font>
                  </TextArea>
                  <TextField id="inputChat" fx:id="inputChat" onAction="#enterClick" prefHeight="43.0" prefWidth="473.0" promptText="Enter a message..." GridPane.rowIndex="2" GridPane.valignment="CENTER">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <GridPane.margin>
                        <Insets left="10.0" right="10.0" />
                     </GridPane.margin>
                  </TextField>
                  <Label prefHeight="29.0" prefWidth="46.0" text="Chat" textFill="WHITE" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <font>
                        <Font name="Avenir Next Bold" size="20.0" />
                     </font>
                     <GridPane.margin>
                        <Insets top="10.0" />
                     </GridPane.margin>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </GridPane>
         </bottom>
      </BorderPane>
      <BorderPane fx:id="whiteBoard" GridPane.columnIndex="1">
         <bottom>
            <GridPane id="toolbox" alignment="CENTER">
               <columnConstraints>
                  <ColumnConstraints minWidth="10.0" />
                  <ColumnConstraints minWidth="10.0" />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label id="line" fx:id="lineLabel" alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#onLineClick" prefHeight="25.0" prefWidth="25.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label id="rectangle" fx:id="rectangleLabel" alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#onRectangleClick" prefHeight="25.0" prefWidth="30.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label id="circle" fx:id="circleLabel" onMouseClicked="#onCircleClick" prefHeight="25.0" prefWidth="25.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label id="logout" fx:id="logout" onMouseClicked="#logoutClicked" prefHeight="25.0" prefWidth="25.0" GridPane.columnIndex="6" GridPane.halignment="RIGHT">
                     <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label id="undo" fx:id="undo" onMouseClicked="#onUndoClicked" prefHeight="25.0" prefWidth="25.0" GridPane.columnIndex="4">
                     <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label id="text" fx:id="textLabel" alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#onTextClick" prefHeight="32.0" prefWidth="37.0" text="T" textAlignment="CENTER" GridPane.columnIndex="5" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <font>
                        <Font name="Arial Bold" size="28.0" />
                     </font>
                  </Label>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </BorderPane.margin>
            </GridPane>
         </bottom>
         <center>
            <Canvas fx:id="canvasWhiteBoard" height="614.0" width="464.0">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Canvas>
         </center>
      </BorderPane>
   </children>
</GridPane>
